{
  "env": {
    "development": {
      "cache": false,
      "prefix": "~!!"
    },
    "production": {
      "cache": true,
      "prefix": "!!"
    }
  },
  "cacheTimeout": 259200,
  "redis": {
    "server": "localhost",
    "port": 9079
  },
  "api": {
    "random-question": "http://jservice.io/api/random",
    "4chan": "http://a.4cdn.org"
  },
  "assets": {
    "root": "assets",
    "audio": {
      "root": "audio"
    },
    "images": {
      "root": "assets/images"
    },
    "content": {
      "root": "assets/content",
      "quickrundown": "quickrundown.txt"
    }
  },
  "commands": {
    "remind": {
      "pollingInMin": 10,
      "pollingLoadMultiplier": 1.5
    }
  }
}