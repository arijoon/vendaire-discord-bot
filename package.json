{
  "name": "vandaire",
  "version": "1.0.0",
  "description": "Make vandaire greate again ",
  "main": "build/bootstrap.js",
  "scripts": {
    "start": "npm run watch",
    "compile": "npm run copyFiles & tsc",
    "copyFiles": "copyfiles src/**/*.json dest/",
    "watch": "SET NODE_ENV=development && nodemon --watch *.ts --ignore '*.spec.ts' --ext ts --exec ts-node src/bootstrap",
    "test": "nyc -r=text -i ts-node/register -e .ts mocha -t 5000 --require ts-node/register  --require reflect-metadata/Reflect",
    "test:all": "npm test **/*.spec.ts",
    "setDev": "SET NODE_ENV=development"
  },
  "author": "Arman Yaraee",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.0.0",
    "inversify": "^3.3.0",
    "reflect-metadata": "^0.1.10"
  }
}
