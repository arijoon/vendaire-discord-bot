{
  "name": "vandaire",
  "version": "1.0.0",
  "description": "Make vandaire greate again ",
  "main": "build/bootstrap.js",
  "scripts": {
    "start": "npm run compile && SET NODE_ENV=production && nodemon --exec node build/bootstrap.js",
    "forever": "forever start build/bootstrap.js",
    "unix:prod:run": "npm run compile && export NODE_ENV=production && npm run forever",
    "prod:single": "npm run compile & SET NODE_ENV=production && nodemon --exec node build/bootstrap.js",
    "dev:run": "npm run compile & nodemon --exec node build/bootstrap.js",
    "compile": "npm run clean && tsc && npm run copyFiles",
    "copyFiles": "copyfiles -u 1 src/**/*.json build/",
    "watch": "SET NODE_ENV=development && nodemon --watch *.ts --ignore '*.spec.ts' --ext ts --exec ts-node src/bootstrap",
    "test": "nyc -r=text -i ts-node/register -e .ts mocha -t 5000 --require ts-node/register  --require reflect-metadata/Reflect",
    "test:all": "npm test **/*.spec.ts",
    "clean": "rm -rf build",
    "tsc": "tsc",
    "tsc-validate": "tsc --noEmit",
    "setDev": "SET NODE_ENV=development"
  },
  "author": "Arman Yaraee",
  "license": "MIT",
  "dependencies": {
    "alexa-app": "^4.2.3",
    "cheerio": "^0.22.0",
    "cli-table": "^0.3.1",
    "discord.js": "^11.3.2",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "fuse.js": "^3.2.1",
    "google-translate-token": "^1.0.0",
    "googleapis": "^19.0.0",
    "imdb-api": "^2.2.2",
    "inversify": "^3.3.0",
    "inversify-inject-decorators": "^3.0.1",
    "jimp": "^0.2.27",
    "lodash": "^4.17.4",
    "mathjs": "^4.1.2",
    "moment": "^2.22.2",
    "node-opus": "^0.2.6",
    "optimist": "^0.6.1",
    "opusscript": "0.0.3",
    "prom-client": "^11.2.0",
    "querystring": "^0.2.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "rx": "^4.1.0",
    "rxjs": "^5.3.0",
    "stack-trace": "0.0.10",
    "tough-cookie": "^2.3.4",
    "urijs": "^1.19.1",
    "uws": "^10.148.0",
    "youtube-node": "^1.3.0",
    "ytdl-core": "^0.12.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^5.2.1",
    "@types/node": "^7.0.65",
    "@types/redis": "^2.8.6",
    "@types/rx": "^4.1.1",
    "chai": "^4.1.2",
    "concurrently": "^4.0.1",
    "copyfiles": "^1.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "ts-node": "^6.0.5",
    "typescript": "^2.3.2"
  }
}
